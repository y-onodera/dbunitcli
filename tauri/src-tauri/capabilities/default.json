{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "args": true,
          "cmd": "command",
          "name": "backend/dbunit-cli-sidecar",
          "sidecar": false
        }
      ]
    },
    "shell:allow-open",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:allow-message",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "cli:allow-cli-matches",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "http://localhost:*/"
        }
      ]
    }
  ]
}